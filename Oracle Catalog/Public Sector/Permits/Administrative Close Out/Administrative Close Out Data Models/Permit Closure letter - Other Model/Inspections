SELECT
LNP_RECORD_ID,
LNP_RECORD_KEY,
INSPECTION_TYPE,



/*I BELIEVE THIS IS THE COMPLETED DATE I MIGHT BE WRONG BUT DOUBLE CHECK */

CASE WHEN EXTRACT(MONTH FROM CAST(CAST(INSPECTION_COMPLETE_DTTM AS TIMESTAMP WITH TIME ZONE) AT TIME ZONE 'EST' AS TIMESTAMP)) = 1 THEN 'January'
	 WHEN EXTRACT(MONTH FROM CAST(CAST(INSPECTION_COMPLETE_DTTM AS TIMESTAMP WITH TIME ZONE) AT TIME ZONE 'EST' AS TIMESTAMP)) = 2 THEN 'February'
	 WHEN EXTRACT(MONTH FROM CAST(CAST(INSPECTION_COMPLETE_DTTM AS TIMESTAMP WITH TIME ZONE) AT TIME ZONE 'EST' AS TIMESTAMP)) = 3 THEN 'March'
	 WHEN EXTRACT(MONTH FROM CAST(CAST(INSPECTION_COMPLETE_DTTM AS TIMESTAMP WITH TIME ZONE) AT TIME ZONE 'EST' AS TIMESTAMP)) = 4 THEN 'April'
	 WHEN EXTRACT(MONTH FROM CAST(CAST(INSPECTION_COMPLETE_DTTM AS TIMESTAMP WITH TIME ZONE) AT TIME ZONE 'EST' AS TIMESTAMP)) = 5 THEN 'May'
	 WHEN EXTRACT(MONTH FROM CAST(CAST(INSPECTION_COMPLETE_DTTM AS TIMESTAMP WITH TIME ZONE) AT TIME ZONE 'EST' AS TIMESTAMP)) = 6 THEN 'June'
	 WHEN EXTRACT(MONTH FROM CAST(CAST(INSPECTION_COMPLETE_DTTM AS TIMESTAMP WITH TIME ZONE) AT TIME ZONE 'EST' AS TIMESTAMP)) = 7 THEN 'July'
	 WHEN EXTRACT(MONTH FROM CAST(CAST(INSPECTION_COMPLETE_DTTM AS TIMESTAMP WITH TIME ZONE) AT TIME ZONE 'EST' AS TIMESTAMP)) = 8 THEN 'August'
	 WHEN EXTRACT(MONTH FROM CAST(CAST(INSPECTION_COMPLETE_DTTM AS TIMESTAMP WITH TIME ZONE) AT TIME ZONE 'EST' AS TIMESTAMP)) = 9 THEN 'September'
	 WHEN EXTRACT(MONTH FROM CAST(CAST(INSPECTION_COMPLETE_DTTM AS TIMESTAMP WITH TIME ZONE) AT TIME ZONE 'EST' AS TIMESTAMP)) = 10 THEN 'October'
	 WHEN EXTRACT(MONTH FROM CAST(CAST(INSPECTION_COMPLETE_DTTM AS TIMESTAMP WITH TIME ZONE) AT TIME ZONE 'EST' AS TIMESTAMP)) = 11 THEN 'November'
	 WHEN EXTRACT(MONTH FROM CAST(CAST(INSPECTION_COMPLETE_DTTM AS TIMESTAMP WITH TIME ZONE) AT TIME ZONE 'EST' AS TIMESTAMP)) = 12 THEN 'December'
	 ELSE NULL END 
	 || ' ' || EXTRACT(DAY FROM CAST(CAST(INSPECTION_COMPLETE_DTTM AS TIMESTAMP WITH TIME ZONE) AT TIME ZONE 'EST' AS TIMESTAMP)) 
	 || ', ' || EXTRACT(YEAR FROM CAST(CAST(INSPECTION_COMPLETE_DTTM AS TIMESTAMP WITH TIME ZONE) AT TIME ZONE 'EST' AS TIMESTAMP)) INSPECTION_COMPLETETION_DATE
    

FROM 
PSC_INS_INSPECTION
