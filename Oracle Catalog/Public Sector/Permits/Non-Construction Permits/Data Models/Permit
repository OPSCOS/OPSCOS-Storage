SELECT
-- GENERAL --
    LNP_RECORD_KEY, 
    LNP_RECORD_ID, 
    ATTRIBUTE_CATEGORY,
    DESCRIPTION, 
    
--APPLICANT INFO--
    PERSON_FIRST_NAME || ' ' || PERSON_LAST_NAME AS APPLICANT_NAME,
	ADDRESS1 as APP_ADDRESS_LINE_ONE, 
    CITY || ', '|| STATE || ' ' || POSTAL_CODE AS APP_ADDRESS_LINE_TWO,
	NVL(PHONE_AREA_CODE || '' || PHONE_NUMBER, '') AS APP_PHONE_NUM,
    EMAIL_ADDRESS1 AS EMAIL,		

--ADDITIONAL CONTACT INFORMATION -- 
    EXTN_ATTRIBUTE_CHAR019 AS ALTERNATE_CONTACT_PERSON,
    EXTN_ATTRIBUTE_CHAR023 AS ALTERNATE_PHONE_NUM,
    EXTN_ATTRIBUTE_CHAR024 AS SECONDARY_PHONE_NUMBER,
    EXTN_ATTRIBUTE_CHAR022 AS CORPORATE_INFO,
    
 --ADDITIONAL_INFORMATION--  
    EXTN_ATTRIBUTE_CHAR070 AS CT_REGISTRATION_NUM,--YES 
    EXTN_ATTRIBUTE_CHAR003	AS ADDITIONAL_LOCATION, --YES
------------------------------------------------------------------------------------------    
 -- SERVICE TYPE 
    CASE EXTN_ATTRIBUTE_CHAR004
        WHEN 'Spl' THEN 'Special Event'
        WHEN 'MBV' THEN 'Mobile Vending'
        WHEN 'Film' THEN 'Film Permit'
        ELSE NULL    
    END AS TYPE_OF_SERVICE,
------------------------------------------------------------------------------------------      
--EVENT CATEGORY-- --yes
    CASE EXTN_ATTRIBUTE_CHAR005
         WHEN 'Ath' THEN 'Athletic/Recreation'
         WHEN 'Auction' THEN 'Auction'
         WHEN 'BlockParty' THEN 'Block Party'
         WHEN 'CC' THEN 'Circus/Carnival'
         WHEN 'CP' THEN 'Concert/Performance'
         WHEN 'Demo' THEN 'Demonstration'
         WHEN 'FC' THEN 'Festival/Celebration'
         WHEN 'FR' THEN 'Flag Raising'
         WHEN 'LWG' THEN 'Lathon Wilder Gym'
         WHEN 'MV1' THEN 'Mobile Vending 1 Day'
         WHEN 'MV1Y' THEN 'Mobile Vending 1 Year'
         WHEN 'PP' THEN 'Parade/Procession'
         WHEN 'PPR' THEN 'Park Pavilion Rental'
         WHEN 'PUP' THEN 'Public Use of Park'
         WHEN 'SchoolBus' THEN 'School Bus Parking Non Public School'
         WHEN 'SchoolUse' THEN 'School Use of Park'
         WHEN 'Vending' THEN 'Vending in Parks'
         WHEN 'Volunteer' THEN 'Volunteer Clean Up Event'
         WHEN 'WDG' THEN 'Wedding'
         When 'OT' THEN EXTN_ATTRIBUTE_CHAR063			
         ELSE NULL
    END AS EVENT_CATEGORY,	
      
--PARK--   
    	CASE EXTN_ATTRIBUTE_CHAR008
        	WHEN 'BOP' THEN 'Boccuzzi Park'
            WHEN 'BP' THEN 'Barrett Park'
            When 'CHP' THEN 'Chestnut Hill Park'
            WHEN 'CIP' THEN 'Cove Island Park'
            WHEN 'CMP' THEN 'Czescik Marina Park'
            WHEN 'COL' THEN 'Columbus Park'
            WHEN 'CP' THEN 'Cummings Park'
            WHEN 'CURT' THEN 'Courtland Park'
            WHEN 'CW' THEN 'Carwin Park'
            WHEN 'DOR' THEN 'Dorothy Heroy Park'
            WHEN 'DR' THEN 'Drotar Park'
            WHEN 'FS' THEN 'Fort Stamford'
            WHEN 'GP' THEN 'Gerli Park'
            WHEN 'HAP' THEN 'Haig Avenue Park'
            WHEN 'HAT' THEN 'Hathfield Park'
            WHEN 'HP' THEN 'Harbor Point Commons Park'
            WHEN 'JP' THEN 'Jackie Robinson Park'
            WHEN 'KEPY' THEN 'Kiwanis Park'
            WHEN 'KP' THEN 'Kosciuszko Park'
            WHEN 'LA' THEN 'Latham Park'
            WHEN 'LP' THEN 'Lione Park'
            WHEN 'MCKP' THEN 'McKeithan Park'
            WHEN 'MIAP' THEN 'McKeithan Park'
            WHEN 'MJP' THEN 'Micheal J. Drotar Park'
            WHEN 'MRP' THEN 'Mill River Park'
            WHEN 'MTP' THEN 'Marilyn Trefy Park'
            when 'NEMP' THEN 'Nemolition Park'
            WHEN 'NTP' THEN 'Northrop Patk'
            WHEN 'RHP' THEN 'Rose Hartman Park'
            WHEN 'SCOP' THEN 'Scofieldtown Park'
            WHEN 'SLEEP' THEN 'Sleepy Hollow Park'
            WHEN 'SNP' THEN 'Susan Nabel Park at Chestnut Hill'
            WHEN 'SP' THEN 'Scalzi Park'
            WHEN 'STJ' THEN 'St.John''s Park'
            WHEN 'VP' THEN 'Veteran''s Park'
            WHEN 'WP' THEN 'West Beach/Park'
            ELSE NULL
    END AS CITY_PARK,

-------------------------------------------------------------------------------------------------
 --SCHOOL-- 
    EXTN_ATTRIBUTE_CHAR069 AS NAME_OF_SCHOOL,
  
 --NAME OF EVENT OR VENDOR -- 
  EXTN_ATTRIBUTE_CHAR014 AS NAME_OF_EVENT_OR_VENDOR,
  
  --ITEMS INTEDED TO BE VENTED -- 
	EXTN_ATTRIBUTE_CHAR065 AS ITEMS_INTENDED_TOBE_VENT,	

-------------------------------------------------------------------------------------------------
---EVENT START DATE AND EVENT END DATE
/* THIS IS THE START DATE BUT IT ALSO INCLUDES THE VENDING ONE DAY */

CASE 
    WHEN EXTN_ATTRIBUTE_CHAR004 = 'Film' THEN
        CASE 
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 1 THEN 'January'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 2 THEN 'February'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 3 THEN 'March'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 4 THEN 'April'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 5 THEN 'May'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 6 THEN 'June'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 7 THEN 'July'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 8 THEN 'August'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 9 THEN 'September'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 10 THEN 'October'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 11 THEN 'November'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 12 THEN 'December'
            ELSE NULL 
        END || ' ' ||
        EXTRACT(DAY FROM EXTN_ATTRIBUTE_TIMESTAMP001) || ', ' ||
        EXTRACT(YEAR FROM EXTN_ATTRIBUTE_TIMESTAMP001)

    WHEN EXTN_ATTRIBUTE_CHAR004 = 'Spl' THEN
        CASE 
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 1 THEN 'January'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 2 THEN 'February'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 3 THEN 'March'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 4 THEN 'April'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 5 THEN 'May'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 6 THEN 'June'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 7 THEN 'July'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 8 THEN 'August'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 9 THEN 'September'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 10 THEN 'October'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 11 THEN 'November'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 12 THEN 'December'
            ELSE NULL 
        END || ' ' ||
        EXTRACT(DAY FROM EXTN_ATTRIBUTE_TIMESTAMP001) || ', ' ||
        EXTRACT(YEAR FROM EXTN_ATTRIBUTE_TIMESTAMP001)

    WHEN EXTN_ATTRIBUTE_CHAR005 = 'MV1' THEN
        CASE 
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP003) = 1 THEN 'January'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP003) = 2 THEN 'February'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP003) = 3 THEN 'March'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP003) = 4 THEN 'April'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP003) = 5 THEN 'May'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP003) = 6 THEN 'June'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP003) = 7 THEN 'July'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP003) = 8 THEN 'August'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP003) = 9 THEN 'September'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP003) = 10 THEN 'October'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP003) = 11 THEN 'November'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP003) = 12 THEN 'December'
            ELSE NULL 
        END || ' ' ||
        EXTRACT(DAY FROM EXTN_ATTRIBUTE_TIMESTAMP003) || ', ' ||
        EXTRACT(YEAR FROM EXTN_ATTRIBUTE_TIMESTAMP003)

    ELSE NULL 
END AS EVENT_START_DATE,

-- EVENT END DATE --

CASE 
    WHEN EXTN_ATTRIBUTE_CHAR004 = 'Film' THEN
        CASE 
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 1 THEN 'January'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 2 THEN 'February'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 3 THEN 'March'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 4 THEN 'April'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 5 THEN 'May'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 6 THEN 'June'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 7 THEN 'July'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 8 THEN 'August'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 9 THEN 'September'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 10 THEN 'October'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 11 THEN 'November'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 12 THEN 'December'
            ELSE NULL 
        END || ' ' ||
        EXTRACT(DAY FROM EXTN_ATTRIBUTE_TIMESTAMP002) || ', ' ||
        EXTRACT(YEAR FROM EXTN_ATTRIBUTE_TIMESTAMP002)

    WHEN EXTN_ATTRIBUTE_CHAR004 = 'Spl' THEN
        CASE 
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 1 THEN 'January'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 2 THEN 'February'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 3 THEN 'March'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 4 THEN 'April'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 5 THEN 'May'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 6 THEN 'June'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 7 THEN 'July'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 8 THEN 'August'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 9 THEN 'September'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 10 THEN 'October'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 11 THEN 'November'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 12 THEN 'December'
            ELSE NULL 
        END || ' ' ||
        EXTRACT(DAY FROM EXTN_ATTRIBUTE_TIMESTAMP002) || ', ' ||
        EXTRACT(YEAR FROM EXTN_ATTRIBUTE_TIMESTAMP002)
 ELSE NULL 
END AS EVENT_END_DATE,

-- OTHER IMPORTANT EVENT TIMES --
    EXTN_ATTRIBUTE_CHAR010 AS SETUP_TIME,
    EXTN_ATTRIBUTE_CHAR011 AS BREAKDOWN_TIME,
    EXTN_ATTRIBUTE_CHAR012 AS EVENT_START_TIME,
    EXTN_ATTRIBUTE_CHAR013 AS EVENT_END_TIME,
    
---ISSUE DATE 
 CASE 
        WHEN EXTRACT(MONTH FROM ISSUE_DATE) = 1 THEN 'January'
        WHEN EXTRACT(MONTH FROM ISSUE_DATE) = 2 THEN 'February'
        WHEN EXTRACT(MONTH FROM ISSUE_DATE) = 3 THEN 'March'
        WHEN EXTRACT(MONTH FROM ISSUE_DATE) = 4 THEN 'April'
        WHEN EXTRACT(MONTH FROM ISSUE_DATE) = 5 THEN 'May'
        WHEN EXTRACT(MONTH FROM ISSUE_DATE) = 6 THEN 'June'
        WHEN EXTRACT(MONTH FROM ISSUE_DATE) = 7 THEN 'July'
        WHEN EXTRACT(MONTH FROM ISSUE_DATE) = 8 THEN 'August'
        WHEN EXTRACT(MONTH FROM ISSUE_DATE) = 9 THEN 'September'
        WHEN EXTRACT(MONTH FROM ISSUE_DATE) = 10 THEN 'October'
        WHEN EXTRACT(MONTH FROM ISSUE_DATE) = 11 THEN 'November'
        WHEN EXTRACT(MONTH FROM ISSUE_DATE) = 12 THEN 'December'
        ELSE NULL 
    END || ' ' ||
    EXTRACT(DAY FROM ISSUE_DATE) || ', ' ||
    EXTRACT(YEAR FROM ISSUE_DATE) AS ISSUE_DATE,
     

 -- RE-ISSUE DATE (UNSURE IF IT WILL WORK)--
 CASE 
        WHEN EXTRACT(MONTH FROM REISSUE_DATE) = 1 THEN 'January'
        WHEN EXTRACT(MONTH FROM REISSUE_DATE) = 2 THEN 'February'
        WHEN EXTRACT(MONTH FROM REISSUE_DATE) = 3 THEN 'March'
        WHEN EXTRACT(MONTH FROM REISSUE_DATE) = 4 THEN 'April'
        WHEN EXTRACT(MONTH FROM REISSUE_DATE) = 5 THEN 'May'
        WHEN EXTRACT(MONTH FROM REISSUE_DATE) = 6 THEN 'June'
        WHEN EXTRACT(MONTH FROM REISSUE_DATE) = 7 THEN 'July'
        WHEN EXTRACT(MONTH FROM REISSUE_DATE) = 8 THEN 'August'
        WHEN EXTRACT(MONTH FROM REISSUE_DATE) = 9 THEN 'September'
        WHEN EXTRACT(MONTH FROM REISSUE_DATE) = 10 THEN 'October'
        WHEN EXTRACT(MONTH FROM REISSUE_DATE) = 11 THEN 'November'
        WHEN EXTRACT(MONTH FROM REISSUE_DATE) = 12 THEN 'December'
        ELSE NULL 
    END || ' ' ||
    EXTRACT(DAY FROM REISSUE_DATE) || ', ' ||
    EXTRACT(YEAR FROM REISSUE_DATE) AS REISSUE_DATE,
    
    
    -- FILM EVENT START DATE --
    CASE 
        WHEN EXTN_ATTRIBUTE_CHAR004 = 'Spl' THEN NULL
        ELSE 
           CASE 
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 1 THEN 'January'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 2 THEN 'February'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 3 THEN 'March'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 4 THEN 'April'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 5 THEN 'May'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 6 THEN 'June'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 7 THEN 'July'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 8 THEN 'August'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 9 THEN 'September'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 10 THEN 'October'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 11 THEN 'November'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 12 THEN 'December'
            ELSE NULL 
        END || ' ' ||
        EXTRACT(DAY FROM EXTN_ATTRIBUTE_TIMESTAMP001) || ', ' ||
        EXTRACT(YEAR FROM EXTN_ATTRIBUTE_TIMESTAMP001)
    END AS FILM_EVENT_START_DATE,
    
    

    -- FILM EVENT END DATE --
    CASE 
        WHEN EXTN_ATTRIBUTE_CHAR004 = 'Spl' THEN NULL
        ELSE 
            CASE 
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 1 THEN 'January'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 2 THEN 'February'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 3 THEN 'March'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 4 THEN 'April'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 5 THEN 'May'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 6 THEN 'June'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 7 THEN 'July'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 8 THEN 'August'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 9 THEN 'September'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 10 THEN 'October'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 11 THEN 'November'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 12 THEN 'December'
            ELSE NULL 
        END || ' ' ||
        EXTRACT(DAY FROM EXTN_ATTRIBUTE_TIMESTAMP002) || ', ' ||
        EXTRACT(YEAR FROM EXTN_ATTRIBUTE_TIMESTAMP002)
    END AS FILM_EVENT_END_DATE,
    
    

    -- SPECIAL EVENT START DATE --
    CASE 
        WHEN EXTN_ATTRIBUTE_CHAR004 = 'Spl' THEN 
            CASE 
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 1 THEN 'January'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 2 THEN 'February'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 3 THEN 'March'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 4 THEN 'April'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 5 THEN 'May'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 6 THEN 'June'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 7 THEN 'July'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 8 THEN 'August'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 9 THEN 'September'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 10 THEN 'October'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 11 THEN 'November'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP001) = 12 THEN 'December'
            ELSE NULL 
        END || ' ' ||
        EXTRACT(DAY FROM EXTN_ATTRIBUTE_TIMESTAMP001) || ', ' ||
        EXTRACT(YEAR FROM EXTN_ATTRIBUTE_TIMESTAMP001)
        ELSE ' '
    END AS SE_EVENT_START_DATE,
    
    

    -- SPECIAL EVENT END DATE --
    CASE 
        WHEN EXTN_ATTRIBUTE_CHAR004 = 'Spl' THEN 
            CASE 
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 1 THEN 'January'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 2 THEN 'February'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 3 THEN 'March'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 4 THEN 'April'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 5 THEN 'May'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 6 THEN 'June'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 7 THEN 'July'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 8 THEN 'August'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 9 THEN 'September'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 10 THEN 'October'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 11 THEN 'November'
            WHEN EXTRACT(MONTH FROM EXTN_ATTRIBUTE_TIMESTAMP002) = 12 THEN 'December'
            ELSE NULL 
        END || ' ' ||
        EXTRACT(DAY FROM EXTN_ATTRIBUTE_TIMESTAMP002) || ', ' ||
        EXTRACT(YEAR FROM EXTN_ATTRIBUTE_TIMESTAMP002)
        ELSE ' '
    END AS SE_EVENT_END_DATE,
    

 -----------------------------------------------------------------------------------------------
 -----------------------------------------------------------------------------------------------
 /* YEAR FOR 1 YEAR MOBILE VENDING*/
    EXTRACT(YEAR FROM ISSUE_DATE) AS YEAR,
------------------------------------------------------------------------------------------------
------------------------------------------------------------------------------------------------
 /* VEHICLE PASSES */
   -- THIS IS FOR THE FIRST VEHICLE -- 
	EXTN_ATTRIBUTE_CHAR054 AS VEHICLE_MAKE,
    EXTN_ATTRIBUTE_CHAR055 AS VEHICLE_MODEL,
    EXTN_ATTRIBUTE_CHAR056 AS VEHICLE_COLOR,
    EXTN_ATTRIBUTE_CHAR057 AS VEHICLE_PLATE,		
    
    -- THIS IS FOR AN ADDITIONAL VEHICLE -- 
    EXTN_ATTRIBUTE_CHAR059 AS ADD_VEHICLE_MAKE,
    EXTN_ATTRIBUTE_CHAR060 AS ADD_VEHICLE_MODEL,
    EXTN_ATTRIBUTE_CHAR061 AS ADD_VEHICLE_COLOR,
    EXTN_ATTRIBUTE_CHAR057 AS ADD_VEHICLE_PLATE
    

FROM PSC_LNP_RECORD
WHERE LNP_RECORD_ID = :P_LNP_RECORD_ID
ORDER BY CREATION_DATE DESC
