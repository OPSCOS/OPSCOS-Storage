SELECT
LNP_RECORD_KEY,
PARCEL_ID,

ADDRESS1,
CROSSROAD1,
CROSSROAD2,

CASE 
WHEN CROSSROAD2 IS NULL THEN CROSSROAD1
WHEN CROSSROAD2 IS NOT NULL THEN CROSSROAD1 ||' and '|| CROSSROAD2
ELSE NULL
END AS CROSSROADS
    

FROM PSC_LNP_PARCEL_ADDRESS
WHERE PRIMARY_FLAG = 'Y'
AND PRIMARY_TRANS_ADDRESS = 'Y'
