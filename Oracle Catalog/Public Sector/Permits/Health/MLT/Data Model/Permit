SELECT
LNP_RECORD_KEY,
LNP_RECORD_ID,
ATTRIBUTE_CATEGORY,
PERSON_FIRST_NAME || ' ' || PERSON_LAST_NAME APPLICANT_NAME, 
 
EXTN_ATTRIBUTE_CHAR033 AS ESTABLISHMENT_NAME,



'July 1, ' || EXTRACT(YEAR FROM ISSUE_DATE) AS ISSUE_DATE, 


 

'June 30, ' || EXTRACT(YEAR FROM EXPIRATION_DATE) AS EXPIRATION_DATE



FROM PSC_LNP_RECORD
WHERE LNP_RECORD_KEY = :P_LNP_RECORD_KEY
ORDER BY CREATION_DATE DESC
