SELECT
EXTN_ATTRIBUTE_CHAR003 AS ADDITIONAL_OCCURENCE,
EXTN_ATTRIBUTE_CHAR004 AS START_END_TIME,
RECORD_NUMBER,
RECORD_NAME,
EXTN_ATTRIBUTE_NUMBER001,

CASE 
        WHEN EXTRACT(MONTH FROM CAST(EXTN_ATTRIBUTE_TIMESTAMP001  AS TIMESTAMP) AT TIME ZONE 'EST') = 1 THEN 'January'
        WHEN EXTRACT(MONTH FROM CAST(EXTN_ATTRIBUTE_TIMESTAMP001  AS TIMESTAMP) AT TIME ZONE 'EST') = 2 THEN 'February'
        WHEN EXTRACT(MONTH FROM CAST(EXTN_ATTRIBUTE_TIMESTAMP001  AS TIMESTAMP) AT TIME ZONE 'EST') = 3 THEN 'March'
        WHEN EXTRACT(MONTH FROM CAST(EXTN_ATTRIBUTE_TIMESTAMP001  AS TIMESTAMP) AT TIME ZONE 'EST') = 4 THEN 'April'
        WHEN EXTRACT(MONTH FROM CAST(EXTN_ATTRIBUTE_TIMESTAMP001  AS TIMESTAMP) AT TIME ZONE 'EST') = 5 THEN 'May'
        WHEN EXTRACT(MONTH FROM CAST(EXTN_ATTRIBUTE_TIMESTAMP001  AS TIMESTAMP) AT TIME ZONE 'EST') = 6 THEN 'June'
        WHEN EXTRACT(MONTH FROM CAST(EXTN_ATTRIBUTE_TIMESTAMP001  AS TIMESTAMP) AT TIME ZONE 'EST') = 7 THEN 'July'
        WHEN EXTRACT(MONTH FROM CAST(EXTN_ATTRIBUTE_TIMESTAMP001  AS TIMESTAMP) AT TIME ZONE 'EST') = 8 THEN 'August'
        WHEN EXTRACT(MONTH FROM CAST(EXTN_ATTRIBUTE_TIMESTAMP001  AS TIMESTAMP) AT TIME ZONE 'EST') = 9 THEN 'September'
        WHEN EXTRACT(MONTH FROM CAST(EXTN_ATTRIBUTE_TIMESTAMP001  AS TIMESTAMP) AT TIME ZONE 'EST') = 10 THEN 'October'
        WHEN EXTRACT(MONTH FROM CAST(EXTN_ATTRIBUTE_TIMESTAMP001  AS TIMESTAMP) AT TIME ZONE 'EST') = 11 THEN 'November'
        WHEN EXTRACT(MONTH FROM CAST(EXTN_ATTRIBUTE_TIMESTAMP001  AS TIMESTAMP) AT TIME ZONE 'EST') = 12 THEN 'December'
        ELSE NULL
    END || ' ' || EXTRACT(DAY FROM CAST(EXTN_ATTRIBUTE_TIMESTAMP001  AS TIMESTAMP) AT TIME ZONE 'EST') 
    || ', ' || EXTRACT(YEAR FROM CAST(EXTN_ATTRIBUTE_TIMESTAMP001  AS TIMESTAMP) AT TIME ZONE 'EST') AS ADDITIONAL_DATES


	
		



FROM 
PSC_TS_GRID_RECORD
